{
  "name": "jsreplace",
  "displayName": "jsReplace",
  "description": "Powerful find and replace using Javascript's String.replace",
  "version": "0.0.2",
  "publisher": "ulucs",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.replaceText"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.replaceText",
        "title": "Replace Text"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "eslint": "^3.6.0",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  }
}
